{
  "version": "1.0.0",
  "description": "React starter KIT",
  "author": "Benjamin Devaublanc <benjamin.devaublanc@gmail.com> (http://bdevaublanc.io/)",
  "main": "./build/bundle.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:devaublanc/react-starter-kit.git"
  },
  "private": true,
  "scripts": {
    "build": "webpack",
    "deploy": "webpack",
    "start": "webpack-dev-server",
    "test": "ava",
    "cover": "nyc --reporter=lcov --reporter=text --reporter=html npm run test",
    "watch:test": "ava --watch",
    "watch:cover": "nodemon --quiet --watch app --exec npm run cover -s",
    "lint": "eslint . --ext .js --ext .jsx || true"
  },
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "6.3.3",
    "ava": "0.13.0",
    "babel-core": "6.7.2",
    "babel-loader": "6.2.4",
    "babel-polyfill": "6.7.2",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "1.1.1",
    "css-loader": "0.23.1",
    "eslint": "2.4.0",
    "eslint-loader": "1.3.0",
    "eslint-plugin-react": "4.2.1",
    "ignore-styles": "1.2.0",
    "jsdom": "8.1.0",
    "nyc": "6.1.1",
    "postcss-calc": "5.2.0",
    "postcss-color-rebeccapurple": "2.0.0",
    "postcss-custom-media": "5.0.1",
    "postcss-custom-properties": "5.0.0",
    "postcss-inline-svg": "1.3.2",
    "postcss-loader": "0.8.2",
    "react-addons-test-utils": "0.14.7",
    "sinon": "1.17.3",
    "style-loader": "0.13.0",
    "stylelint": "5.0.1",
    "webpack": "1.12.14",
    "webpack-dev-server": "1.14.1",
    "webpack-merge": "0.8.3",
    "webpack-postcss-tools": "1.1.1"
  },
  "ava": {
    "files": [
      "web_modules/**/__tests__/*.test.js"
    ],
    "failFast": true,
    "verbose": true,
    "require": [
      "babel-core/register",
      "ignore-styles",
      "./other/setup-ava-tests.js"
    ],
    "babel": "inherit"
  },
  "dependencies": {
    "react": "0.14.7",
    "react-dom": "0.14.7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/devaublanc/react-starter-kit"
  },
  "homepage": "https://github.com/devaublanc/react-starter-kit",
  "bugs": {
    "url": "https://github.com/devaublanc/react-starter-kit/issues"
  },
  "keywords": [
    "react",
    "reactjs"
  ]
}
